{
  "name": "Knocky QR login",
  "description": "A small extension that allows knocky users to login via scanning a QR code.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["scripting", "cookies"],
  "host_permissions": [
    "https://*.knockout.chat/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  }
}
